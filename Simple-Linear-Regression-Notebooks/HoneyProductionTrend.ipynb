        "                print(\"\\tIn {}, the yearly average honey production is predicted to be {:.2f} gallons.\\n\".format(futureYear, ppFy))\n",
        "                print(\"\\tThis reflects a {:.1f}% decrease from {} to {}.\".format(abs(calcPC(ppCy, ppFy)), currentYear, futureYear))\n",
        "                break\n",
